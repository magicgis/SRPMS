<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Mag" table="mag" schema="" catalog="workflow">
        <id name="magId" column="magId"/>
        <property name="magName" column="magName"/>
        <property name="magSNm" column="magSNm"/>
        <property name="issn" column="issn"/>
        <property name="cn" column="cn"/>
        <property name="magSub" column="magSub"/>
        <property name="fq" column="fq"/>
        <many-to-one name="baseByGradeId" class="entity.Base">
            <column name="gradeId"/>
        </many-to-one>
    </class>
</hibernate-mapping>