<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Confer" table="confer" schema="" catalog="workflow">
        <id name="conferId" column="conferId"/>
        <property name="conferType" column="conferType"/>
        <property name="conferNm" column="conferNm"/>
        <property name="conferTime" column="conferTime"/>
        <property name="conferAddr" column="conferAddr"/>
        <set name="conferpapersByConferId" inverse="true">
            <key>
                <column name="conferId" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="entity.Conferpaper"/>
        </set>
    </class>
</hibernate-mapping>