<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Confer" table="confer" schema="" catalog="workflow">
        <id name="conferId" column="confer_id"/>
        <property name="conferType" column="confer_type"/>
        <property name="conferNm" column="confer_nm"/>
        <property name="conferTime" column="confer_time"/>
        <property name="conferAddr" column="confer_addr"/>
    </class>
</hibernate-mapping>