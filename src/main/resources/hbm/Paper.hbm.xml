<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Paper" table="paper" schema="" catalog="workflow">
        <id name="paperId" column="paperId"/>
        <property name="paperName" column="paperName"/>
        <many-to-one name="baseByUnit" class="entity.Base">
            <column name="unit"/>
        </many-to-one>
    </class>
</hibernate-mapping>