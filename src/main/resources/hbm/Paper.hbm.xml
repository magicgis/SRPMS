<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Paper" table="paper" schema="" catalog="workflow">
        <id name="paperId">
            <column name="paperId" sql-type="char" length="14" not-null="true"/>
        </id>
        <property name="paperName">
            <column name="paperName" sql-type="varchar" length="300"/>
        </property>
    </class>
</hibernate-mapping>