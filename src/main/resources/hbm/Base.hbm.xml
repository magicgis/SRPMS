<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Base" table="base" schema="" catalog="workflow">
        <id name="baseId">
            <column name="baseId" sql-type="char" length="8" not-null="true"/>
        </id>
        <property name="table">
            <column name="table" sql-type="varchar" length="15"/>
        </property>
        <property name="keycode">
            <column name="keycode" sql-type="char" length="4"/>
        </property>
        <property name="value">
            <column name="value" sql-type="varchar" length="50"/>
        </property>
    </class>
</hibernate-mapping>