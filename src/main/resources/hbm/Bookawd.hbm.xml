<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Bookawd" table="bookawd" schema="" catalog="workflow">
        <composite-id mapped="true" class="entity.BookawdPK">
            <key-property name="bkId" column="bkId"/>
            <key-property name="awdId" column="awdId"/>
        </composite-id>
        <many-to-one name="awardByAwdId" class="entity.Award">
            <column name="awdId" not-null="true"/>
        </many-to-one>
        <many-to-one name="bookByBkId" class="entity.Book">
            <column name="bkId" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>