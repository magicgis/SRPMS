<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Staff" table="staff" schema="" catalog="workflow">
        <id name="staId" column="staId"/>
        <property name="staName" column="staName"/>
        <property name="deptId" column="deptId"/>
        <property name="iDcard" column="IDcard"/>
        <property name="rankId" column="rankId"/>
        <property name="edu" column="edu"/>
        <property name="position" column="position"/>
        <property name="degree" column="degree"/>
        <property name="staSNm" column="staSNm"/>
        <many-to-one name="baseByDeptId" class="entity.Base">
            <column name="deptId"/>
        </many-to-one>
        <many-to-one name="baseByRankId" class="entity.Base">
            <column name="rankId"/>
        </many-to-one>
        <set name="accountsByStaId" inverse="true">
            <key>
                <column name="staId" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="entity.Account"/>
        </set>
        <set name="bookstasByStaId" inverse="true">
            <key>
                <column name="staId" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="entity.Booksta"/>
        </set>
        <set name="paperstasByStaId" inverse="true">
            <key>
                <column name="staId" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="entity.Papersta"/>
        </set>
    </class>
</hibernate-mapping>