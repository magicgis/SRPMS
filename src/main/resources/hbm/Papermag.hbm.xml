<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Papermag" table="papermag" schema="" catalog="workflow">
        <id name="idpm" column="pm_id"/>
        <property name="colType" column="col_type"/>
        <property name="volIss" column="vol_iss"/>
        <property name="bgPage" column="bg_page"/>
        <property name="paperScore" column="paper_score"/>
        <property name="pubDate" column="pub_date"/>
        <many-to-one name="paperByPaperId" class="entity.Paper">
            <column name="paper_id" not-null="true"/>
        </many-to-one>
        <many-to-one name="magByMagId" class="entity.Mag">
            <column name="mag_id" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>