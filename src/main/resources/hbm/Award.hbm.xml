<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Award" table="award" schema="" catalog="workflow">
        <id name="awdId">
            <column name="awdId" sql-type="char" length="4" not-null="true"/>
        </id>
        <property name="awdType">
            <column name="awdType" sql-type="varchar" length="20"/>
        </property>
        <property name="awdRank">
            <column name="awdRank" sql-type="varchar" length="8"/>
        </property>
    </class>
</hibernate-mapping>