<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Conferpaper" table="conferpaper" schema="" catalog="workflow">
        <composite-id mapped="true" class="entity.ConferpaperPK">
            <key-property name="conferId">
                <column name="conferId" sql-type="char" length="13" not-null="true"/>
            </key-property>
            <key-property name="paperId">
                <column name="paperId" sql-type="char" length="14" not-null="true"/>
            </key-property>
        </composite-id>
        <property name="cpScore">
            <column name="cpScore" sql-type="decimal" precision="4" scale="1"/>
        </property>
        <property name="pubDate">
            <column name="pubDate" sql-type="date"/>
        </property>
    </class>
</hibernate-mapping>