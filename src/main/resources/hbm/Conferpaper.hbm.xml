<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Conferpaper" table="conferpaper" schema="" catalog="workflow">
        <id name="idpc" column="cp_id"/>
        <property name="cpScore" column="cp_score"/>
        <property name="pubDate" column="pub_date"/>
        <many-to-one name="conferByConferId" class="entity.Confer">
            <column name="confer_id" not-null="true"/>
        </many-to-one>
        <many-to-one name="paperByPaperId" class="entity.Paper">
            <column name="paper_id" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>