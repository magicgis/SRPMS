<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Conferpaper" table="conferpaper" schema="" catalog="workflow">
        <composite-id mapped="true" class="entity.ConferpaperPK">
            <key-property name="conferId" column="conferId"/>
            <key-property name="paperId" column="paperId"/>
        </composite-id>
        <property name="cpScore" column="cpScore"/>
        <property name="pubDate" column="pubDate"/>
        <many-to-one name="conferByConferId" class="entity.Confer">
            <column name="conferId" not-null="true"/>
        </many-to-one>
        <many-to-one name="paperByPaperId" class="entity.Paper">
            <column name="paperId" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>