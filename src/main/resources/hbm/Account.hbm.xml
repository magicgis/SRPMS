<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Account" table="account" schema="" catalog="workflow">
        <id name="id">
            <column name="id" sql-type="varchar" not-null="true"/>
        </id>
        <property name="staid">
            <column name="staid" sql-type="char" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar" length="30" not-null="true"/>
        </property>
        <property name="pwd">
            <column name="pwd" sql-type="varchar" length="20" not-null="true"/>
        </property><property name="privilege"><column name="privilege"/></property>
    </class>
</hibernate-mapping>