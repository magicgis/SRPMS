<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="engine.OrderActor" table="ordactor" schema="" catalog="workflow">
        <id name="idoa">
            <column name="idoa" sql-type="varchar" not-null="true"/>
        </id>
        <property name="order">
            <column name="idorder" sql-type="varchar" not-null="true"/>
        </property>
        <property name="actor">
            <column name="idactor" sql-type="varchar" not-null="true"/>
        </property>
        <property name="role">
            <column name="role" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>