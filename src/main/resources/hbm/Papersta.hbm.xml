<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Papersta" table="papersta" schema="" catalog="workflow">
        <composite-id mapped="true" class="entity.PaperstaPK">
            <key-property name="paperId" column="paperId"/>
            <key-property name="staId" column="staId"/>
        </composite-id>
        <property name="paperRole" column="paperRole"/>
        <property name="grScore" column="grScore"/>
        <many-to-one name="paperByPaperId" class="entity.Paper">
            <column name="paperId" not-null="true"/>
        </many-to-one>
        <many-to-one name="staffByStaId" class="entity.Staff">
            <column name="staId" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>