<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Papersta" table="papersta" schema="" catalog="workflow">
        <id name="idps" column="ps_id"/>
        <property name="paperRole" column="paper_role"/>
        <property name="grScore" column="gr_score"/>
        <many-to-one name="paperByPaperId" class="entity.Paper">
            <column name="paper_id" not-null="true"/>
        </many-to-one>
        <many-to-one name="staffByStaId" class="entity.Staff">
            <column name="sta_id" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>