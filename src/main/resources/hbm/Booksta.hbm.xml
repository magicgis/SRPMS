<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Booksta" table="booksta" schema="" catalog="workflow">
        <composite-id mapped="true" class="entity.BookstaPK">
            <key-property name="bkId" column="bkId"/>
            <key-property name="staId" column="staId"/>
        </composite-id>
        <property name="peoWdNum" column="peoWdNum"/>
        <property name="bkcbRole" column="bkcbRole"/>
        <property name="isTx" column="isTx"/>
        <property name="bkScore" column="bkScore"/>
        <many-to-one name="bookByBkId" class="entity.Book">
            <column name="bkId" not-null="true"/>
        </many-to-one>
        <many-to-one name="staffByStaId" class="entity.Staff">
            <column name="staId" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>