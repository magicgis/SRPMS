<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Booksta" table="booksta" schema="" catalog="workflow">
        <id name="idbs" column="bs_id"/>
        <property name="peoWdNum" column="peo_wd_num"/>
        <property name="bkcbRole" column="bkcb_role"/>
        <property name="isTx" column="is_tx"/>
        <property name="bkScore" column="bk_score"/>
        <many-to-one name="bookByBkId" class="entity.Book">
            <column name="bk_id" not-null="true"/>
        </many-to-one>
        <many-to-one name="staffByStaId" class="entity.Staff">
            <column name="sta_id" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>