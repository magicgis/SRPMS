<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Booksta" table="booksta" schema="" catalog="workflow">
        <composite-id mapped="true" class="entity.BookstaPK">
            <key-property name="bkId">
                <column name="bkId" sql-type="char" length="12" not-null="true"/>
            </key-property>
            <key-property name="staId">
                <column name="staId" sql-type="char" length="4" not-null="true"/>
            </key-property>
        </composite-id>
        <property name="peoWdNum">
            <column name="peoWdNum" sql-type="decimal" precision="4" scale="2"/>
        </property>
        <property name="bkcbRole">
            <column name="bkcbRole" sql-type="varchar" length="20"/>
        </property>
        <property name="isTx">
            <column name="isTx" sql-type="tinyint" not-null="true"/>
        </property>
        <property name="bkScore">
            <column name="bkScore" sql-type="decimal" precision="8" scale="4"/>
        </property>
    </class>
</hibernate-mapping>