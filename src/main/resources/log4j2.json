{
  "configuration": {
    "status": "debug",
    "appenders": {
      "appender": [
        {
          "name": "STDOUT",
          "type": "Console",
          "PatternLayout": {
            "pattern": "%d %5p [%t] (%F:%L) - %m%n"
          }
        }
      ]
    },
    "loggers": {
      "root": {
        "level": "debug",
        "AppenderRef": {
          "ref": "STDOUT"
        }
      }
    }
  }
}